<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <ThingDef Abstract="true" Name="LLTextbook_Base">
    <description>Though some might view a textbook as an archaic means of transferring knowledge, this book contains everything one might need to grasp the fundamentals of the subject on the cover. Detailed explanations, practice problems, answer keys, and more. \n\nWhen stored on a shelf in a classroom, students will automatically take relevant study materials to boost the effectiveness of their lessons.</description>
    <thingClass>LifeLessons.Textbook</thingClass>
    <thingCategories>
      <li>LLTextbooks</li>
    </thingCategories>
    <category>Item</category>
    <techLevel>Industrial</techLevel>
    <altitudeLayer>Item</altitudeLayer>
    <alwaysHaulable>true</alwaysHaulable>
    <rotatable>false</rotatable>
    <selectable>true</selectable>
    <pathCost>14</pathCost>
    <stackLimit>1</stackLimit>
    <tradeNeverStack>false</tradeNeverStack>
    <forceDebugSpawnable>true</forceDebugSpawnable>
    <drawerType>MapMeshOnly</drawerType>
    <soundDrop>Standard_Drop</soundDrop>
    <healthAffectsPrice>true</healthAffectsPrice>
    <graphicData>
      <texPath>Items/ClosedBook</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <shaderType>CutoutComplex</shaderType>
    </graphicData>
    <drawGUIOverlay>false</drawGUIOverlay>
    <statBases>
      <MaxHitPoints>50</MaxHitPoints>
      <Mass>0.2</Mass>
      <DeteriorationRate>25</DeteriorationRate>
      <Flammability>1.5</Flammability>
    </statBases>

    <comps>
      <li Class="CompProperties_Forbiddable"/>
      <li Class="LifeLessons.CompProperties_LLBook">
        <openGraphic>
          <texPath>Motes/OpenBook</texPath>
          <graphicClass>Graphic_Single</graphicClass>
          <shaderType>Cutout</shaderType>
        </openGraphic>
      </li>
    </comps>

  </ThingDef>

